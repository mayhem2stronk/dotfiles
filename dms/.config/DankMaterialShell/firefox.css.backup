@media -moz-pref("userChrome.theme-material") {
  :root {
    --md-sys-color-primary: #8ca101;
    --md-sys-color-surface-tint: #8ca101;
    --md-sys-color-on-primary: #f2efdf;
    --md-sys-color-primary-container: #8ca101;
    --md-sys-color-on-primary-container: #5c6a72;
    --md-sys-color-secondary: #dea000;
    --md-sys-color-on-secondary: #f2efdf;
    --md-sys-color-secondary-container: #edeada;
    --md-sys-color-on-secondary-container: #5c6a72;
    --md-sys-color-tertiary: #dea000;
    --md-sys-color-on-tertiary: #f2efdf;
    --md-sys-color-tertiary-container: #edeada;
    --md-sys-color-on-tertiary-container: #5c6a72;
    --md-sys-color-error: #f75552;
    --md-sys-color-on-error: #ffffff;
    --md-sys-color-error-container: #f9dedc;
    --md-sys-color-on-error-container: #410e0b;
    --md-sys-color-background: #fffbef;
    --md-sys-color-on-background: #5c6a72;
    --md-sys-color-surface: #fffbef;
    --md-sys-color-on-surface: #5c6a72;
    --md-sys-color-surface-variant: #fffbef;
    --md-sys-color-on-surface-variant: #5c6a72;
    --md-sys-color-outline: #829181;
    --md-sys-color-outline-variant: #fffbef;
    --md-sys-color-shadow: #000000;
    --md-sys-color-scrim: #000000;
    --md-sys-color-inverse-surface: #1e2326;
    --md-sys-color-inverse-on-surface: #d3c6aa;
    --md-sys-color-inverse-primary: #a7c080;
    --md-sys-color-primary-fixed: #8ca101;
    --md-sys-color-on-primary-fixed: #f2efdf;
    --md-sys-color-primary-fixed-dim: #8ca101;
    --md-sys-color-on-primary-fixed-variant: #f2efdf;
    --md-sys-color-secondary-fixed: #dea000;
    --md-sys-color-on-secondary-fixed: #f2efdf;
    --md-sys-color-secondary-fixed-dim: #dea000;
    --md-sys-color-on-secondary-fixed-variant: #f2efdf;
    --md-sys-color-tertiary-fixed: #dea000;
    --md-sys-color-on-tertiary-fixed: #f2efdf;
    --md-sys-color-tertiary-fixed-dim: #dea000;
    --md-sys-color-on-tertiary-fixed-variant: #f2efdf;
    --md-sys-color-surface-dim: #f2efdf;
    --md-sys-color-surface-bright: #f2efdf;
    --md-sys-color-surface-container-lowest: #fffbef;
    --md-sys-color-surface-container-low: #f2efdf;
    --md-sys-color-surface-container: #fffbef;
    --md-sys-color-surface-container-high: #fffbef;
    --md-sys-color-surface-container-highest: #fffbef;

    --m3-radius: 12px;
    --m3-radius-sm: 10px;
    --m3-elev-0: none;
    --m3-elev-1: 0 1px 2px rgba(0,0,0,.08), 0 1px 3px rgba(0,0,0,.06);
    --m3-elev-2: 0 2px 6px rgba(0,0,0,.10), 0 1px 3px rgba(0,0,0,.06);

    --tab-height: 34px;
    --urlbar-height: 38px;

    --state-hover: color-mix(in srgb, var(--md-sys-color-on-surface) 8%, transparent);
    --state-press: color-mix(in srgb, var(--md-sys-color-on-surface) 12%, transparent);

    --focus-ring: 0 0 0 2px color-mix(in srgb, var(--md-sys-color-primary) 70%, transparent);
  }
}

@media (prefers-color-scheme: dark) {
  :root {
    --md-sys-color-primary: #a7c080;
    --md-sys-color-surface-tint: #a7c080;
    --md-sys-color-on-primary: #1e2326;
    --md-sys-color-primary-container: #a7c080;
    --md-sys-color-on-primary-container: #d3c6aa;
    --md-sys-color-secondary: #7fbbb3;
    --md-sys-color-on-secondary: #1e2326;
    --md-sys-color-secondary-container: #374145;
    --md-sys-color-on-secondary-container: #d3c6aa;
    --md-sys-color-tertiary: #7fbbb3;
    --md-sys-color-on-tertiary: #1e2326;
    --md-sys-color-tertiary-container: #374145;
    --md-sys-color-on-tertiary-container: #d3c6aa;
    --md-sys-color-error: #e57e80;
    --md-sys-color-on-error: #601410;
    --md-sys-color-error-container: #8c1d18;
    --md-sys-color-on-error-container: #f9dedc;
    --md-sys-color-background: #272e33;
    --md-sys-color-on-background: #d3c6aa;
    --md-sys-color-surface: #1e2326;
    --md-sys-color-on-surface: #d3c6aa;
    --md-sys-color-surface-variant: #272e33;
    --md-sys-color-on-surface-variant: #d3c6aa;
    --md-sys-color-outline: #9da9a0;
    --md-sys-color-outline-variant: #272e33;
    --md-sys-color-shadow: #000000;
    --md-sys-color-scrim: #000000;
    --md-sys-color-inverse-surface: #f2efdf;
    --md-sys-color-inverse-on-surface: #5c6a72;
    --md-sys-color-inverse-primary: #8ca101;
    --md-sys-color-primary-fixed: #a7c080;
    --md-sys-color-on-primary-fixed: #1e2326;
    --md-sys-color-primary-fixed-dim: #a7c080;
    --md-sys-color-on-primary-fixed-variant: #1e2326;
    --md-sys-color-secondary-fixed: #7fbbb3;
    --md-sys-color-on-secondary-fixed: #1e2326;
    --md-sys-color-secondary-fixed-dim: #7fbbb3;
    --md-sys-color-on-secondary-fixed-variant: #1e2326;
    --md-sys-color-tertiary-fixed: #7fbbb3;
    --md-sys-color-on-tertiary-fixed: #1e2326;
    --md-sys-color-tertiary-fixed-dim: #7fbbb3;
    --md-sys-color-on-tertiary-fixed-variant: #1e2326;
    --md-sys-color-surface-dim: #272e33;
    --md-sys-color-surface-bright: #414b50;
    --md-sys-color-surface-container-lowest: #272e33;
    --md-sys-color-surface-container-low: #1e2326;
    --md-sys-color-surface-container: #2e383c;
    --md-sys-color-surface-container-high: #374145;
    --md-sys-color-surface-container-highest: #414b50;

    --m3-elev-1: 0 1px 2px rgba(0,0,0,.50), 0 1px 3px rgba(0,0,0,.35);
    --m3-elev-2: 0 4px 10px rgba(0,0,0,.55), 0 1px 3px rgba(0,0,0,.35);

    --state-hover: color-mix(in srgb, var(--md-sys-color-on-surface) 6%, transparent);
    --state-press: color-mix(in srgb, var(--md-sys-color-on-surface) 10%, transparent);
  }
}

/* idk if this even works, but the suggestion in the material updated readme bricks symbols no matter how many fallbacks I add */
:root {
  font-family: system-ui, "Inter Variable", -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, sans-serif, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol", "Noto Color Emoji" !important;
}
